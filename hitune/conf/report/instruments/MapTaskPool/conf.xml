<configuration>
    <property>
        <name>analyzerengine</name>
        <value>InstrumentDataflow</value>
        <description></description>
    </property>
    <property>
        <name>outputfilename</name>
        <value>instruments_map.csv</value>
        <description></description>
    </property>
    <property>
        <name>mapred.map.tasks</name>
          <value>20</value>
          <description></description>
    </property>
    <property>
        <name>attemptid</name>
        <value>attempt_${HiTune.analyzer.targetjob.baseid}_m_</value>
        <description></description>
    </property>

    <property>
        <name>HiTune.analyzer.filefilter.pattern</name>
        <value>.*/MAP/${attemptid}.*</value>
        <description></description>
    </property>
    <property>
        <name>phases</name>
        <value><![CDATA[
            <phase>
                <phasename>Mapphase</phasename>
                <stack>org.apache.hadoop.mapred.MapTask.run(New|Old)Mapper</stack>
                <functions>org.apache.hadoop.mapred.MapRunner.run,org.apache.hadoop.mapred.MapTask\$TrackedRecordReader.next,org.apache.hadoop.mapred.MapRunner.run,MapOutputBuffer.flush</functions>
            </phase>
            <phase>
                <phasename>SpillThread</phasename>
                <stack>SpillThread.run</stack>
                <functions>java.io.DataOutputStream.write,org.apache.hadoop.util.QuickSort.sort,org.apache.hadoop.mapred.IFile\$Writer.append,org.apache.hadoop.io.compress.BlockCompressorStream.write,org.apache.hadoop.io.compress.BlockCompressorStream.compress,org.apache.hadoop.io.compress.CompressorStream.compress,org.apache.hadoop.io.compress.CompressorStream.write,org.apache.hadoop.mapred.MapTask\$MapOutputBuffer.sortAndSpill</functions>
            </phase>
            ]]>
        </value>
        <description></description>
    </property>
    <property>
        <name>status</name>
        <value>NEW,RUNNABLE,BLOCKED,TIME_WAITING,WAITING,UNKNOWN</value>
        <description></description>
    </property>
</configuration>
